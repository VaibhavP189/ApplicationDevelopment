package W13_C1_C2_Recursive;

public class FactorialProgram {
	static int fact(int n)
	{
		if((n==0) || (n==1))
			return 1;
		else if(n>=2)
			return (n* fact(n-1));
		return n;
	}
	
	public static void main (String args[])
	{
		int fact = 1;
		int num = 5;
		fact = fact(num);
		System.out.println("Factorial is" +fact);
	}

}
